{
  "manifest_version": 3,
  "name": "Go Shortcuts",
  "description": "Navigate quickly to stored URLs with go/{shortcut} commands.",
  "version": "1.0.0",
  "omnibox": { 
    "keyword": "go" 
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 